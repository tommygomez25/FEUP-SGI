       front="2.0"
       back="4.0"
       up="1.0"
       down="2.3"
       left="4.3"
       right="2.7" />

    <cameras initial="cam1" > 
        <perspective id="stable" 
            angle="60" 
            near="0.1" 
            far="1000" 
            location="1 5 1" 
            target="-5 1 -8" 
        /> 

        <perspective id="cam1" 
            angle="60" 
            near="0.1" 
            far="1000" 
            location="15 15 15" 
            target="0 0 0" 
